{
    "manifest_version": 3,
    "name": "Data Usage Tracker",
    "version": "1.0",
    "author":"sridhar",
    "description": "This app tracks your data usage!",
    "permissions": [
      "webRequest",
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48":"images/48.jpg",
        "96": "images/96.png",
        "128":"images/128.jpg"
      }
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  